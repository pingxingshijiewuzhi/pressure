<view><view class="mobile"><input class="uni-input" type="number" placeholder="输入手机号" data-event-opts="{{[['input',[['__set_model',['','mobile','$event',[]]]]]]}}" value="{{mobile}}" bindinput="__e"/></view><view class="codelist"><input class="uni-input" type="number" placeholder="输入验证码" data-event-opts="{{[['input',[['__set_model',['','captcha','$event',[]]]]]]}}" value="{{captcha}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getcode']]]]}}" hidden="{{!(stus==true)}}" class="getcode" bindtap="__e">{{text}}</view><view hidden="{{!(stus==false)}}" class="text-gray">{{text+"S重新获取"}}</view></view><view class="mobile"><input class="uni-input" type="text" placeholder="输入用户昵称" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" value="{{username}}" bindinput="__e"/></view><view class="mobile"><input class="uni-input" type="password" placeholder="输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><view class="mobile"><input class="uni-input" type="password" placeholder="再次输入密码" data-event-opts="{{[['input',[['__set_model',['','second_pwd','$event',[]]]]]]}}" value="{{second_pwd}}" bindinput="__e"/></view><view class="bottom"><view data-event-opts="{{[['tap',[['gosub']]]]}}" class="gonext" bindtap="__e">注册</view></view></view>