<view class="content"><view class="head flexs1"><view class=" head_title text-white text-center flexs"><image class="head_img" src="../../static/image/tab_icon_back_ty@2x.png" data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e"></image><view class="head_left"><image class="tx" src="{{baseUrl+avatar}}" data-event-opts="{{[['tap',[['onTopPic',['$event']]]]]}}" bindtap="__e"></image><view data-event-opts="{{[['tap',[['onTopPic',['$event']]]]]}}" class="name" bindtap="__e">{{username}}</view></view></view><view class="title text-white ">{{langtype=='CH'?langch.yonghu:langen.yonghu}}</view></view><view class="setcontent"><view class="username flexs3"><view class="list_left">{{langtype=='CH'?langch.username:langen.username}}</view><input class="uni-input" style="text-align:right;" type="text" placeholder="请输入名称" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="username flexs3"><view class="list_left">{{langtype=='CH'?langch.sex:langen.sex}}</view><input class="uni-input" style="text-align:right;" type="text" placeholder="请输入性别" data-event-opts="{{[['input',[['__set_model',['','sex','$event',[]]]]]]}}" value="{{sex}}" bindinput="__e"/></view><view class="username flexs3"><view class="list_left">{{langtype=='CH'?langch.age:langen.age}}</view><input class="uni-input" style="text-align:right;" type="text" placeholder="请输入年龄" data-event-opts="{{[['input',[['__set_model',['','age','$event',[]]]]]]}}" value="{{age}}" bindinput="__e"/></view><view class="bottom text-center"><view data-event-opts="{{[['tap',[['saveprofile',['$event']]]]]}}" class="submit" bindtap="__e">保存</view></view></view></view>